package trainpkg;

public class Route {
	private int start;
	private int end;
	
	public Route(int start, int end) {
		this.setStart(start);
		this.setEnd(end);
	}
	public int getStart() {
		return start;
	}
	public int getEnd() {
		return end;
	}
	public void setStart(int start) {
		this.start = start;
	}
	public void setEnd(int end) {
		this.end = end;
	}

}
